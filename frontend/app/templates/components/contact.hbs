<div class="contactHeader">

</div>
<div class="contactHeader">Contacts<a href="#"><img src="plus-button.png" width="15" height="15" class="plusButton"></a></div>
<div id="contactParent">
{{#each @model as |item|}}
	<a href="#" class="contactDiv" id={{item.id}} {{action this.anAction item.contact_id}}>
		{{#if item.is_active}}
			<img src="on.png" height="15" width="15" class="contactStatusImage">{{item.name}} {{item.surname}}
		{{else}}
			<img src="off.png" height="15" width="15" class="contactStatusImage">{{item.name}} {{item.surname}}
		{{/if}}
	</a>
	{{/each}}
</div>
